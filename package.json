{
  "name": "@liquid-carrot/carrot",
  "version": "0.4.0",
  "description": "A Simple Node.js AI Library for Neural Network",
  "main": "dist/production/index.min.js",
  "types": "build/src/index.d.ts",
  "module": "dist/production/index.min.js",
  "unpkg": "dist/production/index.browser.min.js",
  "jsdelivr": "dist/production/index.browser.min.js",
  "browser": {
    "fs": false,
    "child_process": false
  },
  "scripts": {
    "build_node_dist_dev": "webpack ./build/src/index.js -o ./dist/dev/node.js --mode development --env dev --target node",
    "build_node_dist_prod": "webpack ./build/src/index.js -o ./dist/production/web.js --mode production --env prod --target node",
    "build_web_dist_dev": "webpack ./build/src/index.js -o ./dist/dev/node.js --mode development --env dev --target web",
    "build_web_dist_prod": "webpack ./build/src/index.js -o ./dist/production/web.js --mode production --env prod --target web",
    "deploy:src": "tsc && npm run build_node_dist_dev && npm run build_node_dist_prod && npm run build_web_dist_dev && npm run build_web_dist_prod",
    "deploy:docs": "typedoc --options typedoc.json",
    "test": "mocha --recursive build/test/",
    "check": "npm run check:src && npm run check:test",
    "check:src": "prettier --check \"./src/**/*.ts\"",
    "check:test": "prettier --check \"./test/**/*.ts\"",
    "fix": "prettier --write .",
    "pretest": "tsc",
    "posttest": "npm run check"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liquidcarrot/carrot.git"
  },
  "keywords": [
    "Artificial Intelligence",
    "AI",
    "Deep Learning",
    "DL",
    "Machine Learning",
    "ML",
    "Neural Network",
    "Neural Networks",
    "NN",
    "Convolutional Neural Network",
    "Convolutional Neural Networks",
    "Convolutional NN",
    "Convolutional NNs",
    "CNN",
    "Artificial Neural Network",
    "Artificial Neural Networks",
    "Artificial NN",
    "Artificial NNs",
    "ANN",
    "Recurrent Neural Network",
    "Recurrent Neural Networks",
    "Recurrent NN",
    "Recurrent NNs",
    "RNN",
    "Perceptron",
    "Sigmoid",
    "TanH",
    "ArcTan"
  ],
  "author": "Liquid Carrot <people@liquidcarrot.io> (https://liquidcarrot.io)",
  "contributors": [
    "Luis Ernesto Carbonell <luis@liquidcarrot.io>",
    "Christian George Echevarria <chris@liquidcarrot.io>"
  ],
  "files": [
    "/src",
    "/test",
    "/dist"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liquidcarrot/carrot/issues"
  },
  "homepage": "https://liquidcarrot.io/",
  "dependencies": {
    "activations": "^1.0.14",
    "chai": "^4.2.0",
    "mnist": "^1.1.0",
    "mocha": "^8.1.1",
    "parcel": "^1.12.4",
    "timsort": "^0.3.0",
    "ts-node": "^8.10.2",
    "webpack": "^4.44.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.0",
    "@types/timsort": "^0.3.0",
    "eledoc": "^0.2.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "2.0.5",
    "ts-loader": "^8.0.2",
    "typedoc": "^0.18.0",
    "typescript": "^3.9.7",
    "webpack-cli": "^3.3.12"
  }
}
